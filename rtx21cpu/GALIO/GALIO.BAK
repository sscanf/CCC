;SETF            /A[15] /A[14]  A[13]  A[12] /IOSTB   ;csio3
;SETF            /A[15]  A[14] /A[13] /A[12] /IOSTB   ;csio4
;SETF            /A[15]  A[14] /A[13]  A[12] /IOSTB   ;csio4

SETF RW /IOSTB  ;IORD
SETF /RW /IOSTB ;IORW

